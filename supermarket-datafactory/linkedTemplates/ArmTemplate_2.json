						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2025-04-24T11:28:08",
						"endTime": "2025-04-24T11:35:19",
						"timeZone": "AUS Eastern Standard Time"
					}
				}
			},
			"dependsOn": []
